<Project Sdk="Microsoft.NET.Sdk.Razor">

  <PropertyGroup>
    <TargetFramework>net6.0</TargetFramework>
    <Nullable>enable</Nullable>
    <ImplicitUsings>enable</ImplicitUsings>
    <AddRazorSupportForMvc>true</AddRazorSupportForMvc>
    <PackageId>UDS.Net.Forms</PackageId>
    <Version>1.0.0</Version>
    <Authors>Sanders-Brown Center on Aging</Authors>
    <Description>UDS Forms razor class library</Description>
    <Owners>UK-SBCoA</Owners>
    <PackageDescription>Razor class library for rendering UDS forms</PackageDescription>
    <RepositoryUrl>https://github.com/UK-SBCoA/uniform-data-set-dotnet</RepositoryUrl>
  </PropertyGroup>

  <ItemGroup>
    <FrameworkReference Include="Microsoft.AspNetCore.App" />
  </ItemGroup>
  <ItemGroup>
    <ProjectReference Include="..\UDS.Net.Services\UDS.Net.Services.csproj" />
  </ItemGroup>
  <ItemGroup>
      <SupportedPlatform Include="browser" />
  </ItemGroup>
  <ItemGroup>
    <None Remove="Pages\" />
    <None Remove="Pages\Visits\" />
    <None Remove="Pages\Shared\" />
    <None Remove="Pages\Participations\" />
    <None Remove="Pages\UDS3\" />
  </ItemGroup>
  <ItemGroup>
    <Folder Include="Pages\" />
    <Folder Include="Pages\Visits\" />
    <Folder Include="Pages\Shared\" />
    <Folder Include="Pages\Participations\" />
    <Folder Include="Pages\UDS3\" />
  </ItemGroup>
</Project>
